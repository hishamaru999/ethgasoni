#: import Factory kivy.factory.Factory

<MyPopup@Popup>
    auto_dismiss: False
    title: "Popup Box"

<MyLayout>
    BoxLayout:
        orientation: "vertical"
        size: root.width, root.height

        Label:
            text: "ETH Gas Station"
            font_size: 32

        Button:
            text: "Dismiss"
            font_size: 32
            on_release: Factory.MyPopup().open()